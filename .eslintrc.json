{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "compat"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "settings": {
    "polyfills": ["fetch"]
  },
  "rules": {
    "semi": [2, "never"],
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0,
    "import/no-named-default": 0,
    "no-unexpected-multiline": 2,
    "compat/compat": 2
  }
}
